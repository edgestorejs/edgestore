{
  "name": "@edgestore/react",
  "version": "0.5.0",
  "description": "Upload files with ease from React/Next.js",
  "homepage": "https://edgestore.dev",
  "repository": "https://github.com/edgestorejs/edgestore.git",
  "author": "Ravi <me@ravi.com>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "keywords": [
    "react",
    "nodejs",
    "nextjs",
    "upload",
    "file",
    "image",
    "cdn",
    "edgestore",
    "edge-store"
  ],
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin rollup-plugin-swc3",
    "dev": "pnpm build --watch",
    "codegen:entrypoints": "tsx entrypoints.script.ts",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --ignore-path ../../.gitignore --report-unused-disable-directives src"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./utils": {
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js",
      "default": "./dist/utils/index.js"
    },
    "./shared": {
      "import": "./dist/shared/index.mjs",
      "require": "./dist/shared/index.js",
      "default": "./dist/shared/index.js"
    },
    "./errors": {
      "import": "./dist/errors/index.mjs",
      "require": "./dist/errors/index.js",
      "default": "./dist/errors/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "package.json",
    "utils",
    "shared",
    "errors",
    "!**/*.test.*"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.294.0",
    "@aws-sdk/s3-request-presigner": "^3.294.0",
    "@edgestore/shared": "0.5.0",
    "@panva/hkdf": "^1.0.4",
    "cookie": "^0.5.0",
    "jose": "^4.13.1",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "zod": ">=3.0.0"
  },
  "devDependencies": {
    "@swc/helpers": "0.5.3",
    "@types/cookie": "^0.5.1",
    "@types/node": "^20",
    "@types/uuid": "^9.0.1",
    "next": "^15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5",
    "zod": "3.21.4"
  },
  "gitHead": "a223c4cb8df50e6b64f9db5dc2daf93848748da9"
}

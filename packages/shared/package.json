{
  "name": "@edgestore/shared",
  "version": "0.6.0",
  "description": "Upload files with ease from React/Next.js",
  "homepage": "https://edgestore.dev",
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/edgestorejs/edgestore.git",
    "directory": "packages/shared"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Ravi <me@ravi.com>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "react",
    "nodejs",
    "nextjs",
    "upload",
    "file",
    "image",
    "cdn",
    "edgestore",
    "edge-store"
  ],
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin rollup-plugin-swc3",
    "dev": "pnpm build --watch",
    "codegen:entrypoints": "tsx entrypoints.script.ts",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --ignore-path ../../.gitignore --report-unused-disable-directives src"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "package.json",
    "!**/*.test.*",
    "!**/__tests__"
  ],
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "zod": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "25.0.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "typescript": "5.9.3",
    "zod": "3.25.42"
  },
  "gitHead": "a223c4cb8df50e6b64f9db5dc2daf93848748da9"
}
